// Importsimport mx.transitions.TweenRGB;import mx.transitions.Tween;import mx.transitions.easing.*;// Variable usedvar tl:MovieClip = this;var firstTime:Boolean = true;var blockOver:Boolean = false;var transitioning:Boolean = false;var selectedFeature:Number;var previousFeature:Number;var selectedColor:String= "0xffffff";var previousColor:String = "0xffffff";var howLong:Number;var whichName:String = "";var autoRotating:Boolean = true;var blockColorTween:Tween;//// Testingvar imgPath:String = "../img/";var xmlPath:String = "../swf/";var swfPath:String = "../swf/";//// Live/*var imgPath:String = "/res/img/";var xmlPath:String = "/res/swf/";var swfPath:String = "/res/swf/";*///var nameArray_array:Array = new Array("", "why_cti", "coach_training", "leadership", "for_organizations");var urlArray_array:Array = new Array("", "/why-coaches-training-institute/", "/coach-training/", "/leadership/", "/organizations/");//=========================================================================// Attach Logo//=========================================================================function attachLogo() {	loadMovie(imgPath + "cit_logo_home.jpg", logo_mc);}feature_mc.logo_mc.onRelease = function() {	getURL("/");}attachLogo();//=========================================================================// Set Blocks//=========================================================================function setNav() {	for (var i=1; i<=4; i++) {		var target:MovieClip = feature_mc.nav_mc["nav" + i];		//		target.num = i;		//		//trace("START FRAME: " + i*10);		//		target.onRollOver = function() {			clearInterval(autoRotateID);			clearInterval(rollOutDelayID);			autoRotating = false;			if (selectedFeature != this.num) {				showFeature(this.num);			}		}		target.onRollOut = function() {			//clearInterval(autoRotateID);			rollOutDelayID = setInterval(autoRotate, 5000);		}		target.onRelease = function() {			getURL(urlArray_array[this.num]);			trace(urlArray_array[this.num]);		}	}}//=========================================================================// Set Learn More//=========================================================================function setLearnMore(num) {	trace(num);	feature_mc.learnMore_mc.onRollOver = function() {		clearInterval(autoRotateID);		clearInterval(rollOutDelayID);		//autoRotating = false;	}	feature_mc.learnMore_mc.onRollOut = function() {		clearInterval(autoRotateID);		rollOutDelayID = setInterval(autoRotate, 5000);	}	feature_mc.learnMore_mc.onRelease = function() {		getURL(urlArray_array[num]);		//trace(urlArray_array[num]);		//test_tf.text = urlArray_array[num];	}}//=========================================================================// Opening Load//=========================================================================function openingLoad() {	//var fadeOpeningBackground= new Tween(openingBackground_mc, "_alpha", mx.transitions.easing.None.easeNone, 100, 0, .5, true);	//fadeOpeningBackground.onMotionFinished = function() {		setNav();		randomFeature = (Math.floor(Math.random()*3))+1;		showFeature(randomFeature);	//}}//=========================================================================// Rollover//=========================================================================function showFeature(num) {	//	blockColorTween.stop();	//	setLearnMore(num);	//	trace("AUTO: " + autoRotating);	if (!autoRotating) {		//clearInterval(autoRotateID);		feature_mc.background_mc.gotoAndStop(1);		trace("ROLLOVER!!!!!!!!!!!!!!!!!!!!!!");	} else {		feature_mc.background_mc.gotoAndPlay("out");	}	//	feature_mc.copy_mc.gotoAndStop(1);	//	previousFeature = selectedFeature;	selectedFeature = num;	//	previousColor = selectedColor;	//	switch(num) {		case 1:			selectedColor = "0xDFC045";			break;		case 2:			selectedColor = "0x689BBC";			break;		case 3:			selectedColor = "0xC66931";			break;		case 4:			selectedColor = "0x933A3C";			break;		default:			// todo			break;	}	navColorSelected = new Color(feature_mc.nav_mc["nav" + selectedFeature]);	navColorSelected.setRGB("0xffffff");	//	navColorPrevious = new Color(feature_mc.nav_mc["nav" + previousFeature]);	navColorPrevious.setRGB("0x3A2726");	//	tl.whichName = nameArray_array[num];	//	blockColorTween = new TweenRGB(feature_mc.nav_mc.block_mc, "", Regular.easeInOut, previousColor, selectedColor, .5, true);	blockColorTween.onMotionFinished = function() {		feature_mc.background_mc.gotoAndPlay(2);		if(firstTime) {			autoRotate();			firstTime = false;		}	}	//}//=========================================================================// Show Copy//=========================================================================function showCopy() {	feature_mc.copy_mc.gotoAndStop(whichName);	var fadeCopy= new Tween(feature_mc.copy_mc, "_alpha", mx.transitions.easing.None.easeNone, 0, 100, .5, true);	fadeCopy.onMotionFinished = function() {		feature_mc.copy_mc.mask_mc.gotoAndPlay(2);	}}//=========================================================================// Auto Rotate//=========================================================================function autoRotate() {	autoRotating = true;	clearInterval(rollOutDelayID);	autoRotateID = setInterval(nextFeature, 12000);}function nextFeature() {	if(selectedFeature == 4) {		var num:Number = 1;	} else {		var num:Number = selectedFeature + 1;	}	showFeature(num);}//=========================================================================// Pause Sequence//=========================================================================// Pause Sequencefunction pauseSequence(howLong) {	pauseSequenceID = setInterval(resumeSequence, howLong);}// Resume Sequencefunction resumeSequence() {	clearInterval(pauseSequenceID);	opening_mc.play();}