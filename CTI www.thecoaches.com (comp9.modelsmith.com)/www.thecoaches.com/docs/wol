<%once>
use Math::Round qw(round);
</%once>
<%init>
my $args = $m->request_args();

my $qfun = round( 1.07526 * ($args->{q11} + $args->{q12} + $args->{q13} + $args->{q14}) / 4); 
my $qphys = round( 1.07526 * ($args->{q21} + $args->{q22} + $args->{q23} + $args->{q24}) / 4); 
my $qpers = round( 1.07526 * ($args->{q31} + $args->{q32} + $args->{q33} + $args->{q34}) / 4); 
my $qheal = round( 1.07526 * ($args->{q41} + $args->{q42} + $args->{q43} + $args->{q44} + $args->{q45}) / 5); 
my $qmon = round( 1.07526 * ($args->{q51} + $args->{q52} + $args->{q53} + $args->{q54}) / 4); 
my $qffam = round( 1.07526 * ($args->{q61} + $args->{q62} + $args->{q63} + $args->{q64} + $args->{q65} + $args->{q71} + $args->{q72} + $args->{q73} + $args->{q74}) / 9); 
my $qcar = round( 1.07526 * ($args->{q81} + $args->{q82} + $args->{q83} + $args->{q84} + $args->{q85}) / 5); 
my $qsig = round( 1.01 * ($args->{q91} + $args->{q92} + $args->{q93} + $args->{q94}) / 4); 

</%init>
   
