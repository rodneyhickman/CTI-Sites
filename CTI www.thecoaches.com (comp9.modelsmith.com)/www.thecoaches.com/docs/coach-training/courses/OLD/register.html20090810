<%init>
my $args = $m->request_args();
my $package = $args->{package};
my @months = qw(January February March April May June July August September October November December);
my ($mn,$yr)=(localtime)[4,5];
$mn += 2;
$yr += 1900;
my @month_select;
for(0..9) {
  my $mon = $mn + $_;
  if ($mon > 11) {
    $yr++;
    $mn -= 12;
    $mon -= 12;
  }
  push (@month_select,"$months[$mon] $yr");
}

my $fun_events = $dbh->selectall_arrayref(

