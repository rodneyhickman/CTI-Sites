<%flags>
inherit => undef
</%flags>
<%once>
 use Data::Dumper;
</%once>
<%init>
my ($rows) = $dbh->selectall_arrayref('SELECT start_date_formatted,call_time FROM event_calendar WHERE course_type_id=9',{Columns=>{}});

my $js = "[\n";
foreach my $row (@$rows){
  $js .= join(",\n", 
              map { qq{ "$_->{start_date_formatted} - $_->{call_time}"} } @$rows ); 
}
$js .= "\n]\n";
</%init>
<% $js %>
