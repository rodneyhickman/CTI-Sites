/*
====================================================================
Main
====================================================================
*/

body {
	font: 62.5%; /* Resets 1em to 10px */ 
	line-height: 22px;
	font-family: Helvetica, Arial, Verdana, sans-serif;
/* 	text-align: center;  */
	min-width: 1000px;
	background: url(../img/bg-top.png) repeat-x;
	color: #000;
	margin: 0;
	padding: 0;
}

/*
====================================================================
Layout of main page elements
====================================================================
*/

#wrapper {
	width: 1000px;
	margin: 20px auto 0;
	text-align: left;
}

aside {
	width: 300px;
	float: right;
	display: inline;
}

aside.secondary-aside {
	width: 200px;
	float: left;
	margin: 0 20px 0 0;
}

#main-content {
	width: 680px;
	float: left;
	margin: 0 0px 0 0;
	display: inline;
}

section.community-highlights {
	width: 460px;
	float: left;
	margin: 0 0 0 0;
	display: inline;
}

section.intro-panel {
	width: 680px;
	display: inline;
	margin: 0 0 20px 0;
}

/*
====================================================================
Header
====================================================================
*/

header {
	width: 1000px;
	height: 190px;
	margin: 0 auto;
	position: relative;
}

header {
	width: 1000px;
	height: 190px;
	margin: 0 auto;
	position: relative;
}

header #verison {
  position: absolute;
  left: 0px;
  top: 12px;
  height: 50px;
	font-size: 11px;
}

#verison a, #verison a:visited {
	color: #392d1c;
}

/*- secondary nav (in top bar) ----------*/

header nav#seondary {
  position: absolute;
  left: 168px;
  top: 15px;
  height: 50px;
}

header nav#seondary ul li a {
	color: #392d1c;
}

header nav#seondary li {
	display: block;
	float: left;
	position: relative;
	font-size: 11px;
	font-weight: bold;
	margin: 0 0 0 12px;
}

header nav#seondary li a:link, #header ul.navigation li a:visited {
  color: #392D1C;
  text-decoration: none;
}

/*- messenger status ----------*/

header #messenger-status {
  background: url("../img/sprite-icons.png") repeat scroll -1px -2px transparent;
  font-size: 0.66em;
  height: 30px;
  left: 415px;
  position: absolute;
  top: 6px;
  width: 261px;
}

header #messenger-status span#name {
	display: inline;
  line-height: 3em;
  padding: 0 0 0 50px;
}

header #messenger-status span.members-online {
  display: inline;
  line-height: 3em;
  padding: 0 0 0 5px;
}

/*- search ----------*/

header .search_form {
  height: 35px;
  left: 720px;
  position: absolute;
  top: 9px;
  width: 285px;
}

header div.search_form form fieldset input#search_term {
  background: url("../img/sprite-icons.png") repeat scroll -2px -36px transparent;
  border: medium none;
  font-family: helvetica,arial,sans-serif;
  height: 21px;
  margin: 0;
  padding: 4px 0 0 10px;
  width: 178px;
}

header .search_form input.submit_button {
  background: url("../img/sprite-icons.png") repeat scroll -2px -65px transparent;
  border: medium none;
  height: 22px;
  width: 84px;
  cursor: pointer;
}

/*- comment ----------*/

header .comment {
  position: absolute;
  right: 0px;
  top: 50px;
  height: 50px;
  font-size: 10px;
}

header .comment a {
	color: #0066CC;
}

header .comment a:hover {
	text-decoration: underline;
}

/*- logo and strapline ----------*/

header h1#branding {
  height: 50px;
  left: 25px;
  position: absolute;
  top: 75px;
  width: 375px;
}

header h2#strapline {
	position: absolute;
  left: 486px;
  top: 92px;
  height: 27px;
  width: 510px;
  font-family: "Gill Sans MT", Arial, Helvetica, sans-serif;
  font-size: 20px;
  font-style: italic;
  z-index: 100;
}

header span {
	display: none;
}

/*
====================================================================
Navigation 
====================================================================
*/

header nav#primary {
	background: url("../img/nav.png") repeat scroll 0 -175px transparent;
	height: 35px;
	list-style-type: none;
	margin: 0;
	padding: 0;
	position: absolute;
	right: 0;
	top: 156px;
	width: 999px;
	z-index: 99;
	display: inline;
}

header nav#primary li { 
  float: left;
}

header nav#primary li a {
	position: absolute;
	top: 0;
	margin: 0;
	padding: 0;
	display: block;
	height: 35px;
	background: url("../img/nav-logged-in.png") no-repeat;
	text-indent: -9999px;
	overflow: hidden;
}

/*up state*/
header nav#primary ul li.home a {
	left: 0;
	width: 116px;
	background-position: 0 0;
}

header nav#primary ul li.members a {
  background-position: -120px 0;
  left: 117px;
  width: 146px;
}

header nav#primary ul li.groups a {
  background-position: -262px 0;
  left: 260px;
  width: 135px;
}

header nav#primary ul li.personal-space a {
  background-position: -397px 0;
  left: 395px;
  width: 195px;
}

header nav#primary ul li.sign-out a {
  background-position: -866px 0;
  left: 866px;
  width: 133px;
}

/*up state*/
header nav#primary ul li.home a:hover {
	left: 0;
	width: 116px;
	background-position: 0 -35px;
}

header nav#primary ul li.members a:hover {
  background-position: -120px -35px;
  left: 117px;
  width: 146px;
}

header nav#primary ul li.groups a:hover {
  background-position: -262px -35px;
  left: 260px;
  width: 135px;
}

header nav#primary ul li.personal-space a:hover {
  background-position: -397px -35px;
  left: 395px;
  width: 195px;
}

header nav#primary ul li.sign-out a:hover {
  background-position: -866px -35px;
  left: 866px;
  width: 133px;
}

/*
====================================================================
Footer
====================================================================
*/

/*- stick it to the bottom ----------*/

* {
	margin: 0;
}
html, body {
	height: 100%;
}
.wrapper {
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -70px; /* the bottom margin is the negative value of the footer's height */
}
.footer, .push {
	height: 70px; /* .push must be the same height as .footer */
	clear: both;
}

